<?xml version="1.0" encoding="UTF-8"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:RDF="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">
    
    <RDF:Description about="urn:mozilla:install-manifest">

        <em:id>FirePHPExtension-Build@firephp.org</em:id>
        <em:name>FirePHP</em:name>
        <em:version>%%VERSION%%</em:version>
        <em:type>2</em:type>
    
        <em:creator>Christoph Dorn</em:creator>
        <em:description>Firebug Extension for PHP based AJAX Development</em:description>

        <em:homepageURL>http://www.firephp.org/</em:homepageURL>
        <em:iconURL>chrome://firephp/skin/FirePHP_32.png</em:iconURL>
        <em:optionsURL></em:optionsURL>

        <em:targetApplication>
            <RDF:Description>
                <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
                <em:minVersion>5.0</em:minVersion>
                <em:maxVersion>8.*</em:maxVersion>
            </RDF:Description>
        </em:targetApplication>
		
        <!-- NOTE: Do not move these tags above <em:targetApplication>. It will break the registration process! -->
        <em:updateURL>%%UPDATE_URL%%</em:updateURL>
        <em:updateKey>%%UPDATE_KEY%%</em:updateKey>
        
    </RDF:Description>
</RDF>